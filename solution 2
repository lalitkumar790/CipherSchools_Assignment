#include<bits/stdc++.h>
using namespace std;

int pow(int x, unsigned int y)
{
    if( y == 0)
        return 1;
    if (y%2 == 0)
        return pow(x, y/2)*pow(x, y/2);
    return x*pow(x, y/2)*pow(x, y/2);
}
  
int order(int x)
{
    int n = 0;
    while (x)
    {
        n++;
        x = x/10;
    }
    return n;
}
  

bool isArmstrong(int x)
{
  
    int n = order(x);
    int z = x, sum = 0;
    while (z)
    {
        int r = z%10;
        sum += pow(r, n);
        z = z/10;
    }
  
    
    return (sum == x);
}
  

int main()
{
    int x = 1253;
    cout << isArmstrong(x) << endl;
    x = 1634;
    cout << isArmstrong(x) << endl;
    return 0;
}
